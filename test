local a=game:GetService("Players").LocalPlayer;local b=game:GetService("RunService")local c=loadstring(game:HttpGet("https://raw.githubusercontent.com/freshdotmp3/UI-Library/refs/heads/main/LXVBk38MGb"))()local c=c:CreateWindow({Title="Zombie Attack",Icon="",Author="By isntthatfresh",Folder="Freshs Script",Size=UDim2.fromOffset(580,460),Transparent=false,Theme="Dark",SideBarWidth=170,HasOutline=true,ToggleKey=Enum.KeyCode.F4})local d={GeneralTab=c:Tab({Title="General",Icon="house",Desc="No description for this tab."})}c:SelectTab(1)local function c()local b,c=nil,math.huge;for d,d in pairs(workspace.enemies:GetChildren())do if d:FindFirstChild("Head")or d:FindFirstChildOfClass("Humanoid").Health>0 then local a=(a.Character.HumanoidRootPart.Position-d.HumanoidRootPart.Position).Magnitude;if a<c then c=a;b=d end end end;for d,d in pairs(workspace.BossFolder:GetChildren())do if d:FindFirstChild("Head")or d:FindFirstChildOfClass("Humanoid").Health>0 then local a=(a.Character.HumanoidRootPart.Position-d.HumanoidRootPart.Position).Magnitude;if a<c then c=a;b=d end end end;return b end;local function e()for a,a in pairs(a.Backpack:GetChildren())do if a:IsA("Tool")and a:FindFirstChild("GunController")then return a end end;return nil end;local function f(a)local a=a;while true do a,k=string.gsub(a,"^(-?%d+)(%d%d%d)",'%1,%2')if(k==0)then break end end;return a end;function convertToMinutes(a)local b=math.floor(a/60)local a=a%60;return b..":"..string.format("%02d",a)end;d.GeneralTab:Section({Title="Infomation"})local g=d.GeneralTab:Code({Title="Game Infomation",Code="None"})local h=d.GeneralTab:Code({Title="Players Infomation",Code="None"})spawn(function()while wait()do pcall(function()g:SetCode("Wave: "..f(tonumber(string.match(a.PlayerGui.MainInterface.MenuHolder.StatsMain.CurrentWave.Text,"%d+"))).."\nGame Time: "..convertToMinutes(tonumber(string.match(a.PlayerGui.MainInterface.MenuHolder:WaitForChild("StatsMain"):WaitForChild("TimeLeftBackground"):WaitForChild("TextLabel").Text,"%d+"))).."\nZombies Amount: "..#game:GetService("Workspace").enemies:GetChildren())h:SetCode("Health: "..f(math.floor(a.Character.Humanoid.Health)).."/"..f(math.floor(a.Character.Humanoid.MaxHealth)).."\n\nLevel: "..f(a.leaderstats.Level.Value).."\nKill: "..f(a.leaderstats.Kills.Value).."\nMoney: "..f(a.leaderstats.Cash.Value))end)end end)d.GeneralTab:Section({Title="Grinding"})d.GeneralTab:Toggle({Title="Auto Farm",Default=false,Callback=function(a)_G.AutoFarmZombie=a end})d.GeneralTab:Toggle({Title="Auto Accept Rewards",Default=false,Callback=function(a)_G.AutoAcceptReward=a end})d.GeneralTab:Section({Title="Miscellaneous"})d.GeneralTab:Toggle({Title="Auto Collect Drops",Default=false,Callback=function(a)_G.AutoCollectDrops=a end})spawn(function()b.Heartbeat:Connect(function()pcall(function()if _G.AutoFarmZombie==true then if#a.Backpack:GetChildren()>0 then if c()==nil then a.Character.HumanoidRootPart.CFrame=CFrame.new(65,-6,2458)else if a.Character.Humanoid.Health>0 then if a.Character:FindFirstChildOfClass("Tool")then if a.Character:FindFirstChildOfClass("Tool"):FindFirstChild("GunController")then a.Character.HumanoidRootPart.CFrame=c().HumanoidRootPart.CFrame*CFrame.new(0,15,15)a.Character.Humanoid.Sit=false else a.Character.HumanoidRootPart.CFrame=CFrame.new(65,-6,2458)a.Character.Humanoid:EquipTool(e())end elseif not a.Character:FindFirstChildOfClass("Tool")then a.Character.HumanoidRootPart.CFrame=CFrame.new(65,-6,2458)a.Character.Humanoid:EquipTool(e())end end end else a.Character.HumanoidRootPart.CFrame=CFrame.new(65,-6,2458)end end end)end)end)spawn(function()while wait()do pcall(function()if _G.AutoFarmZombie==true then if c()~=nil and a.Character:FindFirstChildOfClass("Tool")then game:GetService("ReplicatedStorage"):WaitForChild("Gun"):FireServer(table.unpack({[1]={["Normal"]=Vector3.new(0,0,0),["Direction"]=c().Head.Position,["Name"]=a.Character:FindFirstChildOfClass("Tool").Name,["Hit"]=c().Head,["Origin"]=c().Head.Position,["Pos"]=c().Head.Position}}))end end end)end end)spawn(function()while wait(50)do pcall(function()if _G.AutoAcceptReward==true and _G.AutoFarmZombie==true then for a=1,100 do wait(0.1)game:GetService("ReplicatedStorage"):WaitForChild("RemoteEventContainer"):WaitForChild("CommunicationF"):InvokeServer(table.unpack({[1]="claimReward",[2]=a}))end end end)end end)spawn(function()b.Heartbeat:Connect(function()pcall(function()if _G.AutoCollectDrops==true then if#workspace.Powerups:GetChildren()>0 then for b,b in pairs(workspace.Powerups:GetDescendants())do if b:FindFirstChildOfClass("TouchTransmitter")then b.CFrame=a.Character.HumanoidRootPart.CFrame end end end end end)end)end)spawn(function()b.Stepped:Connect(function()if _G.AutoFarmZombie==true then pcall(function()if not a.Character.HumanoidRootPart:FindFirstChild("Noclip")then local b=Instance.new("BodyVelocity")b.Name="Noclip"b.Parent=a.Character.HumanoidRootPart;b.MaxForce=Vector3.new(1/0,1/0,1/0)b.Velocity=Vector3.new(0,0,0)end;for a,a in pairs(a.Character:GetChildren())do if a:IsA("BasePart")then a.CanCollide=false end end end)else pcall(function()if a.Character.HumanoidRootPart:FindFirstChild("Noclip")then a.Character.HumanoidRootPart["Noclip"]:Destroy()end end)end end)end)spawn(function()b.Heartbeat:Connect(function()pcall(function()if _G.AutoFarmZombie==true then if not a.Character:FindFirstChild("Fresh Highlight")then local b=Instance.new("Highlight")b.Name="Fresh Highlight"b.Parent=a.Character;b.DepthMode=Enum.HighlightDepthMode.AlwaysOnTop;b.Enabled=true;b.FillColor=Color3.fromRGB(255,153,255)b.FillTransparency=0.5;b.OutlineColor=Color3.fromRGB(255,255,255)b.OutlineTransparency=0 end elseif _G.AutoFarmZombie==false then if a.Character:FindFirstChild("Fresh Highlight")then a.Character["Fresh Highlight"]:Destroy()end end end)end)end)
